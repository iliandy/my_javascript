{
  "expo": {
    "name": "UdaciFitness",
    "slug": "UdaciFitness",
    "description": "RN triathlon training app.",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "0.9.0",
    "orientation": "portrait",
    "icon": "https://img.icons8.com/color/512/000000/triathlon.png",
    "notification": {
      "icon": "https://img.icons8.com/color/48/000000/triathlon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.udacifitness.exp",
      "supportsTablet": true
    },
    "android": {
      "package": "org.udacifitness.exp"
    }
  }
}
